    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.21.0"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.4.1",
    "terser": "^5.24.0",
    "vite": "^5.0.0"
  }
